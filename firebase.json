{
  "functions": [
    {
      "source": ".",
      "codebase": "default",
      "runtime": "python311"
    }
  ],
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "**",
        "function": {
          "functionId": "flask_app"
        }
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
} 